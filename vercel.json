{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js",
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "x-vercel-api-request-timeout": "300000"
        }
      }
    ], "functions": {
      "app/api/**/*": {
        "maxDuration": 90 
      }
    }
}