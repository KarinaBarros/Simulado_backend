{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js",
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "x-vercel-request-timeout": "300000"
        }
      }
    ]
}