{
    "version": 2,
    
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js",
        "headers": {
          "Access-Control-Allow-Origin": "*"
          
        }
      },
      {
        "src": "/simulado",
        "dest": "simulado.js"
      }
    ], "functions": {
      "index.js": {
        "maxDuration": 60 
      }
    }
}