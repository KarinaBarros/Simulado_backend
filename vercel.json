{
    "version": 2,
    "functions": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "simulado.js": {
          "maxDuration": 60 
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js",
        "headers": {
          "Access-Control-Allow-Origin": "*"

        }
      }
    ]
}